            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PersistableData": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "24-digit hex string",
            "example": "000000000000000000000000"
          },
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrderType": {
        "enum": [
          "None",
          "Stock",
          "Transit",
          "Wholesale",
          "Retail",
          "StockAction",
          "TransitAction",
          "WholesaleAction",
          "RetailAction",
          "Action"
        ],
        "type": "string"
      },
      "OrderDto": {
        "required": [
          "orderNumber",
          "packingList"
        ],
        "type": "object",
        "properties": {
          "palletFrom": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "palletTo": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "palletCount": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "orderNumber": {
            "type": "string"
          },
          "packingList": {
            "type": "string"
          },
          "weight": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "cost": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "boxCount": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "type": {
            "$ref": "#/components/schemas/OrderType"
          }
        },
        "additionalProperties": false
      },
      "ProductType": {
        "enum": [
          "Food",
          "NonFood",
          "Alco",
          "Fresh",
          "Freeze"
        ],
        "type": "string"
      },
      "CreateReservationSlotRequestDto": {
        "required": [
          "palletCount",
          "pickUpDateFrom",
          "slotId"
        ],
        "type": "object",
        "properties": {
          "slotId": {
            "type": "string",
            "format": "24-digit hex string",
            "example": "000000000000000000000000"
          },
          "pickUpDateFrom": {
            "type": "string",
            "format": "date-time",
            "example": "2020-12-30T08:02:37"
          },
          "pickUpDateTo": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "example": "2020-12-30T08:02:37"
          },
          "timeSlotFrom": {
            "type": "string",
            "format": "date-span",
            "nullable": true
          },
          "timeSlotTo": {
            "type": "string",
            "format": "date-span",
            "nullable": true
          },
          "warehouseId": {
            "type": "string",
            "format": "24-digit hex string",
            "example": "000000000000000000000000"
          },
          "palletCount": {
            "maximum": 1000,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "boxCount": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "weight": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "cost": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "orders": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderDto"
            },
            "nullable": true
          },
          "productTypeId": {
            "$ref": "#/components/schemas/ProductType"
          }
        },
        "additionalProperties": false
      },
      "ReservationSlotResponseDto": {
        "type": "object",
        "properties": {
          "pickUpDateFrom": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "example": "2020-12-30T08:02:37"
          },
          "pickUpDateTo": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "example": "2020-12-30T08:02:37"
          },
          "consolidationDate": {
            "type": "string",
            "format": "date",
            "example": "2020-12-30"
          },
          "availableUntil": {
            "type": "string",
            "format": "date-time",
            "example": "2020-12-30T08:02:37"
          },
          "timeSlotFrom": {
            "type": "string",
